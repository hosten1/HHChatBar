#import "LDSysMsgModel+Extend.h"

@implementation LDSysMsgModel (Extend)

-(void)updateSysMsgModel:(LDSysMsgModel*)msgModel{
    if(msgModel.ID){
        self.ID = msgModel.ID;
    }
    if (msgModel.timestamp) {
        self.timestamp = msgModel.timestamp;
    }
    if (msgModel.userID) {
        self.userID = msgModel.userID;
    }
    if (msgModel.groupID) {
        self.groupID = msgModel.groupID;
    }
    if (msgModel.msgType) {
        self.msgType = msgModel.msgType;
    }
    if (msgModel.isResponse) {
        self.isResponse = msgModel.isResponse;
    }
    if (msgModel.isRead) {
        self.isRead = msgModel.isRead;
    }
    if (msgModel.opType){
        self.opType = msgModel.opType;
    }
    if (msgModel.subType){
        self.subType = msgModel.subType;
    }
    if (msgModel.userName) {
        self.userName = msgModel.userName;
    }
    if (msgModel.groupName){
        self.groupName = msgModel.groupName;
    }
    if (msgModel.portraitUrl){
        self.portraitUrl = msgModel.portraitUrl;
    }
    if (msgModel.addInfo) {
        self.addInfo = msgModel.addInfo;
    }
}

@end
