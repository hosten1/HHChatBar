#import "LDRoomModel+Extend.h"

@implementation LDRoomModel (Extend)

- (void)createRoom:(void (^) (NSError *error))completion
{
    objc_setAssociatedObject(self, "create", completion, OBJC_ASSOCIATION_RETAIN_NONATOMIC);
    [SDKClient sendRequestToSDK:imsdkldd::ldd_logic_cmd_createRoom postData:self completion:nil];
}

- (void)removeRoom:(void (^) (NSError *error))completion
{
    objc_setAssociatedObject(self, "remove", completion, OBJC_ASSOCIATION_RETAIN_NONATOMIC);
    [SDKClient sendRequestToSDK:imsdkldd::ldd_logic_cmd_delRoom postData:self completion:nil];
}

@end
