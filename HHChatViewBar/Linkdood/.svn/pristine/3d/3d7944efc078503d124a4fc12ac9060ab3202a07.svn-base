#import "LDNoteModel.h"

@implementation LDNoteModel

+(instancetype)buildWithTitle:(NSString *)title
                      content:(NSString *)content
                      keyword:(NSString *)keyword
                         type:(NoteType)type
                   sourceType:(NoteSource)sourceType{
    LDNoteModel *note = [[LDNoteModel alloc]init];
    note.title = title;
    note.content = content;
    note.keyword = keyword;
    note.type = type;
    note.sourceType = sourceType;
    return note;
}

@end
